{
  "name": "react-line-awesome-svg",
  "version": "1.0.9",
  "description": "Use line-awesome as react svg component",
  "main": "index.js",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "test": "jest",
    "build": "npm run build:clean && npm run build:tsc",
    "build:clean": "rimraf ./components && rimraf ./icons ",
    "build:tsc": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "np --no-yarn --no-publish --no-cleanup",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williampsena/react-line-awesome"
  },
  "keywords": [
    "react",
    "line-awesome"
  ],
  "dependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@svgr/cli": "^5.4.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.5.1",
    "jest": "^25.5.4",
    "line-awesome": "^1.3.0",
    "np": "^6.2.3",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "license": "MIT"
}
