{
  "name": "react-line-awesome",
  "version": "1.0.0",
  "description": "Use line-awesome as react svg component",
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "test": "jest",
    "build": "npm run build:clean && npm run build:tsc && npm run build:static",
    "build:clean": "rimraf ./dist ",
    "build:tsc": "tsc",
    "build:static": "rm -rf ./dist/__tests__",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "np --no-yarn --no-publish --no-cleanup",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williampsena/react-line-awesome"
  },
  "keywords": [
    "react",
    "line-awesome"
  ],
  "dependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@svgr/cli": "^5.4.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "jest": "^25.5.4",
    "line-awesome": "^1.3.0",
    "np": "^6.2.3",
    "react-test-renderer": "^16.13.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
